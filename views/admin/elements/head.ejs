<script>
function parseNumber(num)
    {
        let arr = [];
        while(num >= 1000){
        
            arr.unshift(num%1000 == 0 ? '000': num%1000 < 100? '0'+num%1000 : num%1000);
            num= parseInt(num/1000);
        }
        arr.unshift(num%1000);
        return arr.join(',');
    }
</script>
